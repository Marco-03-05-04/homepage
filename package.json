{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "bourbon": "^5.1.0",
    "gatsby": "^2.0.0",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-remark-prismjs": "^3.0.2",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.1.3",
    "node-sass": "^4.9.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.0.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-icons": "npx svgstore --inline src/images/icons/*.svg > src/images/icons/icons-sprite.svg && npx svgr --no-svgo src/images/icons/icons-sprite.svg > src/images/icons/IconsSprite.jsx",
    "build-svg": "npx svgr src/images/top-solution/logo-text.svg > src/images/top-solution/logo-text.jsx",
    "build": "gatsby build",
    "deploy": "npm run build && npm run s3-sync && npm run cloudfront-invalidate",
    "s3-sync": "aws s3 sync ./public s3://topsolution/website --acl public-read --exclude \"*DS_Store\" --delete --profile topsolution-bucket",
    "cloudfront-invalidate": "aws cloudfront create-invalidation --distribution-id E3O0GC5RWFQTF7 --paths /index.html --profile topsolution-bucket",
    "start": "gatsby develop",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@svgr/cli": "^3.0.0",
    "prettier": "^1.14.2",
    "svgstore-cli": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
