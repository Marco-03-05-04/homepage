{
  "name": "top-solution-homepage",
  "description": "Top Solution srl website",
  "version": "1.0.0",
  "author": "Luca Barbetti <l.barbetti@topsolution.it>",
  "dependencies": {
    "bourbon": "^7.0.0",
    "gatsby": "^2.20.9",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "node-sass": "^4.13.1",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-visibility-sensor": "^5.1.1",
    "rehype-react": "^5.0.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-icons": "npx svgstore --inline src/images/icons/*.svg > src/images/icons/icons-sprite.svg && npx svgr --no-svgo src/images/icons/icons-sprite.svg > src/images/icons/IconsSprite.jsx",
    "build-svg": "npx svgr src/images/top-solution/logo-text.svg > src/images/top-solution/logo-text.jsx",
    "build": "gatsby build",
    "deploy:dev": "npm run build && TS_HOME_S3_PATH=s3://topsolution/website-dev npm run s3-sync && TS_HOME_CLOUDFRONT_ID=E3BPM5YSN13FES npm run cloudfront-invalidate",
    "deploy:prod": "npm run build && TS_HOME_S3_PATH=s3://topsolution/website npm run s3-sync && TS_HOME_CLOUDFRONT_ID=E3O0GC5RWFQTF7 npm run cloudfront-invalidate",
    "s3-sync": "aws s3 sync ./public $TS_HOME_S3_PATH --acl public-read --exclude \"*DS_Store\" --delete --profile topsolution-bucket",
    "cloudfront-invalidate": "aws cloudfront create-invalidation --distribution-id $TS_HOME_CLOUDFRONT_ID --paths /index.html /robots.txt /sitemap.xml --profile topsolution-bucket",
    "start": "gatsby develop",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@svgr/cli": "^5.3.0",
    "prettier": "^2.0.2",
    "svgstore-cli": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
