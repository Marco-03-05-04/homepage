{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-components": "rollup -c",
    "build-pages": "sh build-pages.sh",
    "build": "npm run build-components && npm run build-pages",
    "watch-components": "rollup -c -w",
    "watch-pages": "npm run build-pages && chokidar 'src/pages/*.html' -c 'node build-page.js {path}'",
    "serve": "npx http-server -s -c-1 ./public",
    "start": "concurrently \"npm run watch-components\" \"npm run watch-pages\" \"npm run serve\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "fdir": "^5.2.0",
    "handlebars": "^4.7.7",
    "http-server": "^14.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.47.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.10.1"
  },
  "dependencies": {
    "@material/mwc-snackbar": "^0.25.3",
    "@material/mwc-textfield": "^0.25.3",
    "lodash.debounce": "^4.0.8",
    "simplex-noise": "^3.0.0"
  }
}
